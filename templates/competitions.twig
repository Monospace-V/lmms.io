{% block foot %}
<script type="text/javascript">
$(document).ready(function(){
    $(".comp-header").click(function(){
		var target = $(this).attr('data-target');
		$(target).collapse('toggle');
		$(this).children('svg').toggleClass('rotate');
		// only load the Bandcamp widget when it should be visible
		var bc_widget = $(target).children('div');
		var bc_iframe = $(bc_widget).children('iframe');
		if (!$(bc_iframe).attr('src')) {
			$(bc_iframe).attr('src', $(bc_widget).attr('data-href'));
		}
	});
});
</script>
